  "scripts": {
    "start-unsec": "npm run copy:unsecure-env && SET PORT=3000 && react-scripts start",
    "start": "set HTTPS=true&&set PORT=8762 && npm run copy:secure-env &&react-scripts start  ",
    "start:local": "set HTTPS=true&&set PORT=8762 && npm run copy:local &&react-scripts start  ",
    "start2": "set HTTPS=true&&set PORT=8762 && npm run copy:secure-env &&react-app-rewired start  ",
    "start3": "set HTTPS=true&&set PORT=8762 && react-scripts start",
    "build-unsecure": "react-scripts build",
    "copy:unsecure-env": "xcopy /Y .env.unsecure.env .env",
    "copy:local": "xcopy /Y .env.local .env",
    "copy:secure-env": "xcopy /Y .env.secure.env .env",
    "build": "npm run build:secure",
    "build:secure": "SET REACT_APP_HTTP=https && SET REACT_APP_PORT_GRAPHQL=4001 && SET REACT_APP_PORT1_REST=3002  && react-scripts build",
    "copy": "npm run copy:secure",
    "copy:unsecure": "xcopy /s /Y build \\\\nlbavwixs\\dashboard",
    "copy:secure": "xcopy /s /Y build \\\\nlbavwixs.infor.com\\dashboard.dev",
    "deploy": "npm run deploy:secure",
    "deploy:secure": "SET NODE_ENV=production && npm run copy:secure-env && npm run build:secure && npm run copy:secure",
    "deploy:unsecure": "npm run copy:unsecure-env && npm run build && npm run copy:unsecure",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "testenv": "SET NODE_ENV=test",
    "prodenv": "SET NODE_ENV=production",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/app.css --env production",
    "build:css2": "tailwindcss build src/styles/tailwind.css -o src/styles/app.css "
